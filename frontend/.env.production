# KeLiva Frontend Production Environment Variables

# Backend API URL (Production)
VITE_API_URL=https://keliva.onrender.com

# Voice Configuration
VITE_USE_WEB_SPEECH_API=true
VITE_DEFAULT_VOICE=en-US-AriaNeural

# WebSocket URL (Production)
VITE_WS_URL=wss://keliva.onrender.com/ws

# Security Configuration
VITE_MAX_FILE_SIZE=5242880
VITE_ALLOWED_FILE_TYPES=image/jpeg,image/png,image/gif,audio/wav,audio/mp3
VITE_SESSION_TIMEOUT=1800000
VITE_ENABLE_CSP=true

# Production Settings
VITE_DEBUG_MODE=false
VITE_LOG_LEVEL=error
VITE_ENVIRONMENT=production